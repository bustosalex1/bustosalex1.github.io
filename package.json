{
    "name": "website",
    "type": "module",
    "version": "0.3",
    "packageManager": "pnpm@9.4.0",
    "scripts": {
        "stamp": "echo \"COMMIT_HASH=$(git rev-parse --short HEAD)\"> .env && echo \"WEBSITE_VERSION=$(node -p \"require('./package.json').version\")\">>.env",
        "dev": "astro dev",
        "start": "astro dev",
        "build": "pnpm stamp && astro build",
        "preview": "astro preview",
        "astro": "astro",
        "format": "prettier . --write",
        "lint": "prettier --check . && eslint . && svelte-check --tsconfig ./tsconfig.json"
    },
    "dependencies": {
        "@fontsource/fira-code": "^5.2.5",
        "fuse.js": "^7.1.0",
        "katex": "^0.16.21"
    },
    "devDependencies": {
        "@astrojs/mdx": "^4.2.1",
        "@astrojs/svelte": "^7.0.7",
        "@astrojs/ts-plugin": "^1.10.4",
        "@iconify-json/carbon": "^1.2.8",
        "@iconify-json/line-md": "^1.2.5",
        "@iconify-json/mdi": "^1.2.3",
        "@iconify-json/octicon": "^1.2.5",
        "@tailwindcss/typography": "^0.5.16",
        "@tailwindcss/vite": "^4.0.15",
        "@types/eslint": "^9.6.1",
        "astro": "^5.5.4",
        "autoprefixer": "^10.4.21",
        "eslint": "^9.23.0",
        "eslint-config-prettier": "^10.1.1",
        "eslint-plugin-svelte": "^3.3.3",
        "globals": "16.0.0",
        "prettier": "^3.5.3",
        "prettier-plugin-astro": "^0.14.1",
        "prettier-plugin-svelte": "^3.3.3",
        "rehype-katex": "^7.0.1",
        "remark-math": "^6.0.0",
        "sharp": "^0.33.5",
        "svelte": "^5.25.3",
        "svelte-check": "^4.1.5",
        "tailwindcss": "^4.0.15",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.27.0",
        "unplugin-icons": "^22.1.0",
        "vite": "^6.2.2"
    }
}
