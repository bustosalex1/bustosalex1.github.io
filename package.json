{
    "name": "website",
    "type": "module",
    "version": "0.3",
    "packageManager":"pnpm@9.4.0",
    "scripts": {
        "stamp": "echo \"COMMIT_HASH=$(git rev-parse --short HEAD)\"> .env && echo \"VERSION=0.3\">>.env",
        "dev": "astro dev",
        "start": "astro dev",
        "build": "pnpm stamp && astro build",
        "preview": "astro preview",
        "astro": "astro",
        "format": "prettier . --write",
        "lint": "prettier --check . && eslint . && svelte-check --tsconfig ./tsconfig.json"
    },
    "dependencies": {
        "@astrojs/mdx": "^3.1.3",
        "@astrojs/svelte": "^5.7.0",
        "@astrojs/tailwind": "^5.1.0",
        "@astrojs/ts-plugin": "^1.9.0",
        "astro": "^4.12.2",
        "daisyui": "^4.12.10",
        "fuse.js": "^7.0.0",
        "katex": "^0.16.11",
        "rehype-katex": "^7.0.0",
        "remark-math": "^6.0.0",
        "svelte": "^4.2.18",
        "tailwindcss": "^3.4.6",
        "theme-change": "^2.5.0"
    },
    "devDependencies": {
        "@iconify-json/carbon": "^1.1.36",
        "@iconify-json/line-md": "^1.1.38",
        "@iconify-json/octicon": "^1.1.56",
        "@tailwindcss/typography": "^0.5.13",
        "@types/eslint": "^9.6.0",
        "autoprefixer": "^10.4.19",
        "eslint": "^9.7.0",
