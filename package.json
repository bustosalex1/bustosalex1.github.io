        "eslint-plugin-svelte": "^2.43.0",
        "globals": "15.0.0",
        "postcss": "^8.4.39",
        "prettier": "^3.3.3",
        "prettier-plugin-astro": "^0.14.1",
        "prettier-plugin-svelte": "^3.2.6",
        "sharp": "^0.33.4",
        "svelte-check": "^3.8.4",
        "typescript": "^5.5.4",
        "typescript-eslint": "8.0.0-alpha.20",
        "unplugin-icons": "^0.19.0",
        "vite": "^5.3.4"
